            walkX = -self.memory.getData("walkX")/10.0
            walkY = -self.memory.getData("walkY")/10.0
            turnAround = -self.memory.getData("turnAround")/10.0
            self.motionProxy.setWalkTargetVelocity(walkX,walkY,turnAround,self.frequency )]]>
