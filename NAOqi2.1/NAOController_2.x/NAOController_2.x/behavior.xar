                                                        <![CDATA[import socket
import struct
import time
class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.memory = ALProxy("ALMemory")
        self.motion = ALProxy("ALMotion")
        self.managerProxy = ALProxy("ALBehaviorManager")
        try:
            self.audio = ALProxy("ALAudioDevice")
        except :
            self.audio = ALProxy("ALAudioPlayer")
    def onLoad(self):
        self.bstart = True
    def onUnload(self):
        self.bstart = False
        #self.sock.close()
    def onInput_onStart(self):
        self.SendPort = self.getParameter("SendPort")
        self.RecvPort = self.getParameter("RecvPort")
        self.threadtime = self.getParameter("threadtime")
        self.memory.insertData("behaviorNumber",14)
        self.startSocket()
        while(self.bstart):
            try:
                self.msg,(self.addr, self.port) = self.sock.recvfrom( 20 )
                self.logger.info("get recv")
                self.msg_unpack()
            except :
                self.logger.info("bug????")
                pass
    def onInput_onStop(self):
        self.onUnload()
        pass
    def startSocket(self):
        self.desc = ('<broadcast>',self.SendPort)
        self.sock = socket.socket( socket.AF_INET, socket.SOCK_DGRAM )
        self.sock.setsockopt(socket.SOL_SOCKET,socket.SO_REUSEADDR,1)
        self.sock.setsockopt(socket.SOL_SOCKET,socket.SO_BROADCAST,1)
        self.sock.bind( ('',self.RecvPort))
    def msg_unpack(self):
        if self.msg == "REQUEST" and self.bstart:
            self.sendMsg()
    def MsgPack(self):
        header = "SHIQISHAN"
        try:
            stiffness = self.motion.getStiffnesses("Body")
            volume = self.audio.getOutputVolume()/10
            isRobotics = 1
        except :
            isRobotics = 0
            stiffness = 0
            volume = 0
        behaviorList = self.managerProxy.getInstalledBehaviors()
        runingList = self.managerProxy.getRunningBehaviors()
        runingBehaviors = ""
        installBehaviors = ""
        for i in runingList:
            i = i.rstrip("/.")
            runingBehaviors += i
            runingBehaviors += "$"
        for i in behaviorList:
            if i.count(".currentChoregrapheBehavior"):
                pass
            elif i.count(".lastUploadedChoregrapheBehavior"):
                pass
            elif i.count("animation"):
                pass
            elif i.count("default"):
                pass
            elif i.count("boot-config"):
                pass
            elif i.count("diagnostic"):
                pass
            else:
                installBehaviors += i
                installBehaviors += "$"
        self.roboticsInfo = struct.pack('!9s3i3500s6000s',header,isRobotics,stiffness,volume,runingBehaviors,installBehaviors)
    def sendMsg(self):
        self.MsgPack()
        self.sock.sendto(self.roboticsInfo,(self.addr, self.port))]]>
</content>
